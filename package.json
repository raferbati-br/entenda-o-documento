{
  "name": "entenda-o-documento",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    
    "lint": "eslint",
   
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    
    "test:load": "node scripts/run-load-test.mjs",

    "test:coverage": "npm run test:unit:coverage && npm run test:e2e:coverage && node scripts/report-coverage-summary.mjs",
    "test:e2e:coverage": "set E2E_COVERAGE=1&& set NEXT_DISABLE_TURBOPACK=1&& playwright test && node scripts/report-playwright-coverage.mjs",
    "test:unit:coverage": "node scripts/run-unit-coverage.mjs",
    
    "bdd:coverage": "npm run bdd:coverage:load && npm run bdd:coverage:e2e && node scripts/report-bdd-summary.mjs",
    "bdd:coverage:load": "node scripts/check-load-coverage.mjs",    
    "bdd:coverage:e2e": "node scripts/check-bdd-coverage.mjs"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@upstash/redis": "^1.36.1",
    "framer-motion": "^12.23.26",
    "idb-keyval": "^6.2.2",
    "next": "^16.1.6",
    "openai": "^6.15.0",
    "posthog-js": "^1.170.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.2.0",
    "jsdom": "^27.4.0",
    "typescript": "^5",
    "v8-to-istanbul": "^9.3.0",
    "vitest": "^4.0.17"
  }
}
